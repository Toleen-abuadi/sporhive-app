{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "check:i18n": "node scripts/check-i18n.js",
        "check:types": "tsc --noEmit",
    "check:lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "check:format": "prettier --check .",
    "check:unused": "ts-prune",
    "check:deps": "depcheck",

    "check:i18n": "node scripts/check-i18n.js",
    "check:i18n:missing": "node scripts/check-i18n-missing.js",
    "check:i18n:variables": "node scripts/check-i18n-vars.js",
    "check:i18n:rtl": "node scripts/check-hardcoded-strings.js",

    "check:navigation": "node scripts/check-navigation.js",
    "check:replace-usage": "grep -R \"router.replace\" ./src",
    "check:dead-routes": "node scripts/check-dead-routes.js",

    "check:safe-area": "grep -R \"SafeAreaView\" ./src",
    "check:scroll": "grep -R \"ScrollView\" ./src",
    "check:keyboard": "grep -R \"KeyboardAvoidingView\" ./src",

    "check:images": "grep -R \"<Image\" ./src",
    "check:cover": "node scripts/check-images-cover.js",

    "check:rerenders": "node scripts/check-rerenders.js",
    "check:memo": "grep -R \"React.memo\" ./src",
    "check:callbacks": "node scripts/check-inline-callbacks.js",

    "check:tokens": "grep -R \"token\" ./src",
    "check:console": "grep -R \"console.log\" ./src",
    "check:env": "node scripts/check-env.js",

    "check:all": "npm run check:types && npm run check:lint && npm run check:i18n && npm run check:navigation && npm run check:images"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@formatjs/intl-pluralrules": "^6.2.1",
    "@logrocket/react-native": "^1.62.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.3.16",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "expo": "~54.0.33",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.10",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "i18next": "^23.10.1",
    "lottie-react-native": "^7.3.4",
    "lucide-react-native": "^0.544.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^14.1.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "cross-env": "^10.1.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  }
}
