{
  "expo": {
    "name": "SporHive",
    "slug": "SporHive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "sporhive",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.sporhive.app",
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      "expo-notifications"
    ],
    "extra": {
      "router": {
        "origin": "sporhive://",
        "links": [
          {
            "path": "r/:token"
          },
          {
            "path": "r/:token"
          }
        ]
      },
      "API_BASE_URL": "",
      "eas": {
        "projectId": "d8e46091-2c57-4cbe-bdb9-a1a56df7e54d"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}